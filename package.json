{
	"name": "twitchat",
	"version": "7.18.1",
	"private": true,
	"scripts": {
		"dev": "concurrently --kill-others \"npm run front/serve\" \"npm run server/watch\"",
		"build": "npm run i18n && npm run front/build && npm run server/build",
		"front/serve": "vite --host",
		"front/build": "vue-tsc --noEmit && vite build",
		"server/watch": "tsc -w -p ./tsconfig_server.json",
		"server/build": "tsc -p ./tsconfig_server.json",
		"server/start": "pm2 start bootstrap-pm2.json",
		"patch": "npm version patch -m \"Upgrade version to v%s\"",
		"minor": "npm version minor -m \"Upgrade version to v%s\"",
		"major": "npm version major -m \"Upgrade version to v%s\"",
		"i18n": "node src_labels/index.js",
		"lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"streamdeck_package": "npm run streamdeck_package_clean && DistributionTool.exe -b -i streamdeck_plugin/fr.twitchat.sdPlugin -o streamdeck_plugin",
		"streamdeck_package_clean": "shx rm streamdeck_plugin/fr.twitchat.streamDeckPlugin",
		"streamdeck_deploy_local": "shx cp -r streamdeck_plugin/fr.twitchat.sdPlugin C:/Users/franc/AppData/Roaming/Elgato/StreamDeck/Plugins/",
		"check-circular-dependencies": "dpdm ./src_front/main.ts"
	},
	"dependencies": {
		"@fastify/cors": "^8.1.0",
		"@fastify/rate-limit": "^7.5.0",
		"@fastify/static": "^6.5.0",
		"@popperjs/core": "^2.11.5",
		"ajv": "^8.12.0",
		"core-js": "^3.8.3",
		"fast-json-patch": "^3.1.1",
		"fastify": "^4.8.1",
		"jsonwebtoken": "^8.5.1",
		"lorem-ipsum": "^2.0.8",
		"node-fetch": "^2.6.7",
		"node-static": "^0.7.11"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.1.0",
		"@types/dom-screen-wake-lock": "^1.0.0",
		"@types/gsap": "^3.0.0",
		"@types/node": "^16.11.25",
		"@types/tmi.js": "^1.8.1",
		"@types/vue-select": "^3.16.0",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vitejs/plugin-vue": "^2.3.3",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-plugin-router": "~5.0.0",
		"@vue/cli-plugin-typescript": "~5.0.0",
		"@vue/cli-service": "~5.0.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"@vue/tsconfig": "^0.1.3",
		"concurrently": "^7.5.0",
		"dpdm": "^3.9.0",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^8.0.3",
		"grapheme-splitter": "^1.0.4",
		"gsap": "^3.10.4",
		"less-loader": "^8.0.0",
		"obs-websocket-js": "^5.0.2",
		"pinia": "^2.0.23",
		"shx": "^0.3.4",
		"tmi.js": "github:Durss/tmi.js",
		"typescript": "^4.8.3",
		"vite": "^2.9.12",
		"vite-plugin-package-version": "^1.0.2",
		"vue": "^3.2.41",
		"vue-class-component": "^8.0.0-0",
		"vue-i18n": "^9.2.2",
		"vue-router": "^4.0.12",
		"vue-select": "^4.0.0-beta.3",
		"vue-tsc": "^1.0.9",
		"vue3-carousel": "^0.2.9",
		"vue3-country-flag-icon": "^0.1.2",
		"vue3-marquee": "^3.0.7",
		"vuedraggable": "^4.1.0"
	}
}
