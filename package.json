{
	"name": "twitchat",
	"version": "5.2.0",
	"private": true,
	"scripts": {
		"dev": "vite --host",
		"serve": "vite --host",
		"patch": "npm version patch -m \"Upgrade version to v%s\"",
		"minor": "npm version minor -m \"Upgrade version to v%s\"",
		"major": "npm version major -m \"Upgrade version to v%s\"",
		"build": "vue-tsc --noEmit && vite build",
		"lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"streamdeck_package": "DistributionTool.exe -b -i streamdeck_plugin/fr.twitchat.sdPlugin -o streamdeck_plugin",
		"check-circular-dependencies": "dpdm ./src/main.ts"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.5",
		"ajv": "^6.12.6",
		"core-js": "^3.8.3",
		"fast-json-patch": "^3.1.1",
		"jsonwebtoken": "^8.5.1",
		"lorem-ipsum": "^2.0.8",
		"node-fetch": "^2.6.6",
		"node-static": "^0.7.11"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.1.0",
		"@types/dom-screen-wake-lock": "^1.0.0",
		"@types/gsap": "^3.0.0",
		"@types/node": "^16.11.25",
		"@types/tmi.js": "^1.8.1",
		"@types/vue-select": "^3.16.0",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vitejs/plugin-vue": "^2.3.3",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-plugin-router": "~5.0.0",
		"@vue/cli-plugin-typescript": "~5.0.0",
		"@vue/cli-service": "~5.0.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"@vue/tsconfig": "^0.1.3",
		"dpdm": "^3.9.0",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^8.0.3",
		"gsap": "^3.10.4",
		"less-loader": "^8.0.0",
		"obs-websocket-js": "^5.0.0-beta.1",
		"tmi.js": "^1.8.5",
		"typescript": "^4.5.4",
		"vite": "^2.9.12",
		"vite-plugin-package-version": "^1.0.2",
		"vue": "^3.2.36",
		"vue-class-component": "^8.0.0-0",
		"vue-router": "^4.0.12",
		"vue-select": "^4.0.0-beta.3",
		"vue-tsc": "^0.34.7",
		"vue3-country-flag-icon": "^0.1.2",
		"vue3-marquee": "^3.0.7",
		"vuedraggable": "^4.1.0",
		"vuex": "^4.0.2"
	}
